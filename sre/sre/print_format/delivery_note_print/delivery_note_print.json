 "html": "<div style=\"font-family: Arial, sans-serif; font-size: 12px; margin: 10px; width: 210mm; height: 297mm;\">\n\n  <!-- Header -->\n  <div style=\"text-align: center; font-weight: bold; font-size: 16px; margin-bottom: 5px;\">\n    <span style=\"background-color: white; padding: 2px 8px; display: inline-block;\">SIDDIQ RENEWABLE ENERGY</span><br>\n    <span style=\"font-size: 14px;\">Pakistan</span>\n  </div>\n\n  <div style=\"display: flex; justify-content: space-between; margin-bottom: 15px; font-weight: bold;\">\n    <div>\n      Gate Pass #: {{ doc.name }}<br>\n     \n    </div>\n    \n     <div class=\"gate-pass-header\">\n    NON-RETURNABLE<br>OUTWARD\n    </div>\n  </div>\n\n  <!-- Details Section -->\n  <table style=\"width: 100%; border-collapse: collapse; margin-bottom: 15px;\">\n    <tr>\n      <td style=\"width: 50%; vertical-align: top; padding: 5px;\">\n        <b>Supplier's Name:</b> {{ doc.customer or \"GENERAL\" }}<br>\n        <b>Carrying By:</b> {{ doc.get(\"carrying_by\") or \"MR HAMZA SRE\" }}<br>\n        <b>Vehicle #:</b> {{ doc.get(\"vehicle_no\") or \"BY HAND\" }}<br>\n        <b>Exp. Date:</b> {{ frappe.utils.formatdate(doc.get(\"exp_date\"), 'dd-mm-yyyy') if doc.get(\"exp_date\") else \"\" }}\n      </td>\n      <td style=\"width: 50%; vertical-align: top; padding: 5px; text-align: right;\">\n        <b>Remarks:</b> {{ doc.get(\"remarks\") or \"SIDDIQ LEATHER WORKS\" }}<br>\n        <b>Department:</b> {{ doc.set_warehouse }}<br>\n        <b>Bilty Date:</b> {{ frappe.utils.formatdate(doc.get(\"bilty_date\"), 'dd-mm-yyyy') if doc.get(\"bilty_date\") else \"\" }}<br>\n        <b>Bilty No:</b> {{ doc.get(\"bilty_no\") or \"\" }}\n      </td>\n    </tr>\n  </table>\n\n  <!-- Items Table -->\n  <table style=\"width: 100%; border: 1px solid #000; border-collapse: collapse; font-size: 12px; margin-bottom: 20px;\">\n    <thead>\n      <tr style=\"background-color: #f0f0f0;\">\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 8%;\">S.#</th>\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 30%;\">Item Description</th>\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 30%;\">Description</th>\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 10%;\">Item Size</th>\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 11%;\">Quantity</th>\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 11%;\">Unit</th>\n      </tr>\n    </thead>\n    <tbody>\n      {% for item in doc.items %}\n      <tr>\n        <td style=\"border: 1px solid #000; padding: 5px; text-align: center;\">{{ loop.index }}</td>\n        <td style=\"border: 1px solid #000; padding: 5px;\">{{ item.item_code }} {% if item.item_name %}({{ item.item_name }}){% endif %}</td>\n        <td style=\"border: 1px solid #000; padding: 5px;\">{{ doc.get(\"remarks\") or \"SIDDIQ LEATHER WORKS\" }}</td>\n        <td style=\"border: 1px solid #000; padding: 5px; text-align: center;\">{{ item.get(\"item_size\") or \"\" }}</td>\n        <td style=\"border: 1px solid #000; padding: 5px; text-align: center;\">{{ item.qty }}</td>\n        <td style=\"border: 1px solid #000; padding: 5px; text-align: center;\">{{ item.uom }}</td>\n      </tr>\n      {% endfor %}\n      <tr>\n        <td colspan=\"4\" style=\"border: 1px solid #000; padding: 5px; text-align: right;\"><b>Total:</b></td>\n        <td style=\"border: 1px solid #000; padding: 5px; text-align: center;\"><b>{{ doc.total_quantity or doc.total_qty }}</b></td>\n        <td style=\"border: 1px solid #000; padding: 5px;\"></td>\n      </tr>\n    </tbody>\n  </table>\n\n  <!-- Signatures -->\n  <div style=\"margin-top: 30px; display: flex; justify-content: space-between; font-size: 11px; font-weight: bold;\">\n    <div>Prepared By: __________________</div>\n    <div>Checked By: __________________</div>\n    <div>Approved By: __________________</div>\n    <div>Receiver's Sign: __________________</div>\n  </div>\n\n  <!-- Separator -->\n  <hr style=\"border: none; border-top: 1px dashed #000; margin: 30px 0;\">\n\n  <!-- Office Copy -->\n  \n  \n  <!-- Repeated Header for Office Copy -->\n  <div style=\"text-align: center; font-weight: bold; font-size: 16px; margin-bottom: 5px;\">\n    <span style=\"background-color: white; padding: 2px 8px; display: inline-block;\">SIDDIQ RENEWABLE ENERGY</span><br>\n    <span style=\"font-size: 14px;\">Pakistan</span>\n  </div>\n<div style=\"text-align: right; font-weight: bold; font-size: 14px; margin-bottom: 10px;\">Office Copy</div>\n  <div style=\"display: flex; justify-content: space-between; margin-bottom: 15px; font-weight: bold;\">\n    <div>\n      Gate Pass #: {{ doc.name }}<br>\n     \n    </div>\n    <div class=\"gate-pass-header\">\n    NON-RETURNABLE<br>OUTWARD\n    </div>\n  </div>\n\n  <!-- Simplified Office Copy Details -->\n  <table style=\"width: 100%; border-collapse: collapse; margin-bottom: 15px;\">\n    <tr>\n      <td style=\"width: 50%; vertical-align: top; padding: 5px;\">\n        <b>Supplier's Name:</b> {{ doc.customer or \"GENERAL\" }}<br>\n        <b>Carrying By:</b> {{ doc.get(\"carrying_by\") or \"MR HAMZA SRE\" }}<br>\n        <b>Vehicle #:</b> {{ doc.get(\"vehicle_no\") or \"BY HAND\" }}\n      </td>\n      <td style=\"width: 50%; vertical-align: top; padding: 5px; text-align: right;\">\n        <b>Remarks:</b> {{ doc.get(\"remarks\") or \"SIDDIQ LEATHER WORKS\" }}<br>\n        <b>Department:</b> {{ doc.set_warehouse }}<br>\n      </td>\n    </tr>\n  </table>\n\n  <!-- Office Copy Items Table -->\n  <table style=\"width: 100%; border: 1px solid #000; border-collapse: collapse; font-size: 12px; margin-bottom: 20px;\">\n    <thead>\n      <tr style=\"background-color: #f0f0f0;\">\n        <th style=\"border: 1px solid #000; padding: 5px; text-align: center; width: 8%;\">S.#</th>\n        <th style=\"b